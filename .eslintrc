{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "defaultParams": true
  },
  "rules": {
    "max-len": 0,
    "quotes": 0,
    "spaced-comment": 0,
    "no-unused-vars": 0,
    "object-curly-spacing": 0,
    "no-duplicate-imports": 0,
    "react/jsx-filename-extension": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "global-require": 0,
    "import/no-unresolved": [2, { "ignore": ["^[/]"] }],
    "import/no-absolute-path": [2, { "ignore": ["^[/]"] }]
  }
}

